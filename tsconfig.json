{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "lib": ["ESNext"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node"
  }
}
